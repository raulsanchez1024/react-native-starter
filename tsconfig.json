{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@common/*": ["src/components/common/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@context/*": ["src/context/*"],
      "@hooks/*": ["src/hooks/*"],
      "@mocks/*": ["src/mocks/*"],
      "@navigators/*": ["src/navigators/*"],
      "@screens/*": ["src/screens/*"],
      "@src/*": ["src/*"],
      "@store/*": ["src/store/*"],
      "@styles/*": ["src/styles/*"],
      "@test/*": ["test/*"],
      "@types": ["src/types"],
      "@utils/*": ["src/utils/*"],
      "@utils": ["src/utils"]
    }
  }
}
